{
    "window.title": "${remoteName}",
    "window.restoreWindows": "none",
    "window.closeWhenEmpty": true,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80
    ],
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.renderControlCharacters": false,
    "workbench.statusBar.visible": false,
    "workbench.iconTheme": "file-icons-mac",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Mariana Pro (Cool)",
    "breadcrumbs.enabled": false,
    "extensions.ignoreRecommendations": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.macOptionIsMeta": true,
    "files.exclude": {
        ".ipynb_checkpoints": true
    },
    "search.exclude": {},
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--disable=R,C",
        "--extension-pkg-whitelist=cv2,torch"
    ],
    "python.pythonPath": "~/.pyenv/shims/python",
    "python.terminal.activateEnvironment": false,
    "[latex]": {
        "editor.wordWrap": "on"
    },
    "[json]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.associations": {
        "**/dotfiles/*": "shellscript"
    },
    "[markdown]": {
        "editor.formatOnSave": false,
        "editor.wordWrap": "on",
    },
    "[plaintext]": {
        "editor.wordWrap": "on"
    },
    "explorer.autoReveal": false,
    "createtmpfile.deleteOnExit": true,
    "createtmpfile.tmpDir": "/tmp",
    "gitlens.currentLine.enabled": false,
    "gitlens.mode.active": "zen",
    "python.showStartPage": false,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
    "java.configuration.checkProjectSettingsExclusions": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.message.update.show": false,
    "latex-workshop.message.error.show": false,
    // "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latex.clean.fileTypes": [
        "__latexindent_temp.tex",
        "*.bcf",
        "*.fls",
        "*.run.xml",
        "*.synctex.gz",
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.fls",
        "*.log",
        "*.fdb_latexmk",
        "*.snm",
        "*.synctex(busy)",
        "*.synctex.gz(busy)",
        "*.nav"
    ],
    "python.languageServer": "None",
    "python.workspaceSymbols.enabled": true,
    "python.workspaceSymbols.exclusionPatterns": [
        "**/site-packages/**",
        "**/.ipynb_checkpoints/**"
    ],
    "cSpell.enabled": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "autoDocstring.docstringFormat": "numpy",
    "workbench.activityBar.visible": false,
    "window.zoomLevel": 2,
    "terminal.integrated.tabs.enabled": true,
    "workbench.editor.untitled.hint": "hidden",
    "python.defaultInterpreterPath": "~/.pyenv/shims/python",
    "editor.inlineSuggest.enabled": true,
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.jupyterServerType": "local",
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.sendSelectionToInteractiveWindow": false,
    "jupyter.askForKernelRestart": false,
    "workbench.colorCustomizations": {
        "[Mariana Pro (Cool)]": {
            "notebook.cellEditorBackground": "#23292e",
            // "notebook.outputContainerBackgroundColor": "#313a42",
            "notebook.focusedCellBorder": "#999999"
        }
    },
    "notebook.cellFocusIndicator": "border",
    "notebook.showCellStatusBar": "visibleAfterExecute",
    "notebook.insertToolbarLocation": "notebookToolbar",
    "notebook.cellToolbarLocation": {
        "default": "hidden",
    },
    "notebook.lineNumbers": "on"
}
