{
    "window.title": "${remoteName}",
    "window.zoomLevel": 2,
    "window.restoreWindows": "none",
    "window.closeWhenEmpty": true,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        80
    ],
    "editor.formatOnSave": true,
    "editor.suggestSelection": "first",
    "editor.renderControlCharacters": false,
    "workbench.statusBar.visible": false,
    "workbench.activityBar.visible": false,
    "workbench.iconTheme": "file-icons-mac",
    "workbench.startupEditor": "welcomePageInEmptyWorkbench",
    "workbench.colorTheme": "Mariana Pro (Cool)",
    "breadcrumbs.enabled": false,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "extensions.ignoreRecommendations": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.macOptionIsMeta": true,
    "files.exclude": {
        ".ipynb_checkpoints": true,
    },
    "search.exclude": {},
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
    },
    "python.formatting.provider": "black",
    "python.dataScience.jupyterServerURI": "local",
    "python.pythonPath": "~/.pyenv/shims/python",
    "python.terminal.activateEnvironment": false,
    "python.dataScience.disableJupyterAutoStart": true,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "python.dataScience.askForKernelRestart": false,
    "[json]": {
        "editor.formatOnSave": false
    },
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.message.update.show": false,
    "files.associations": {
        "*.symlink": "shellscript"
    },
    "[markdown]": {
        "editor.wordWrap": "on"
    },
    "[plaintext]": {
        "editor.wordWrap": "on"
    },
    "explorer.autoReveal": false,
    "createtmpfile.deleteOnExit": true,
    "createtmpfile.tmpDir": "~/.cache/vscode-tempdir",
    "gitlens.currentLine.enabled": false,
    "gitlens.mode.active": "zen",
}
